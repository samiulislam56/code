#include<stdio.h>
int main()
{
    int n;
    printf("Enter an integer number:");
    scanf("%d", &n);
    int a=0;
    for(int i=2; i<n; i++){
        if(n%i==0){
            a=1;
        }
    }
    if(n==1)
        printf("The number is neither prime nor composite");
    else if(a==0)
        printf("The number is prime");
    else
        printf("The number is composite");

    return 0;
}
